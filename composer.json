        "php": ">= 5.4.8",
        "psr/log": "^1.0",
        "symfony/polyfill-php56": "^1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "phpunit/phpunit": "^4.8.35|^5|^6|^7"
